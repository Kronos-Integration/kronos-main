{
  "name": "kronos-main",
  "version": "0.0.0",
  "description": "Base step implementation",
  "main": "lib/adapter-outbound-file.js",
  "scripts": {
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests",
    "doc": "jsdoc lib/*.js",
    "test": "mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-main.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-main/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-main#readme",
  "devDependencies": {
    "asciidoctor.js": "^1.5.2",
    "broccoli": "^0.16.9",
    "broccoli-funnel": "^1.0.1",
    "broccoli-merge-trees": "^1.1.1",
    "keychain": "^1.0.0",
    "node-github": "0.0.3",
    "octokat": "^0.4.15",
    "verb": "^0.8.10"
  },
  "contributors": [{
    "name": "Torsten Link",
    "email": "torstenlink@gmx.de"
  }, {
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=4.2.1"
  },
  "verb": {
    "ignore": [
      ".git"
    ],
    "deps": {
      "include": [
        "readme-includes",
        "readme-badges"
      ]
    },
    "reflinks": {
      "list": [
        "jsdiff",
        "option-cache",
        "template",
        "plasma",
        "config-cache"
      ]
    }
  }
}
