{
  "name": "kronos-main",
  "version": "0.0.0-semantic-release",
  "description": "Umbrella for all Kronos modules",
  "scripts": {
    "cover": "node ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "doc": "node ./node_modules/.bin/jsdoc lib/*.js",
    "test": "node ./node_modules/.bin/mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-main.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-main/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-main#readme",
  "devDependencies": {
    "asciidoctor.js": "1.5.5-1",
    "broccoli": "^0.16.9",
    "broccoli-funnel": "^1.0.3",
    "broccoli-merge-trees": "^1.1.1",
    "github-change-remote-file": "^1.1.0",
    "js-yaml": "^3.6.1",
    "keychain": "^1.0.0",
    "node-github": "0.0.3",
    "octokat": "^0.4.17",
    "verb": "^0.8.10",
    "semantic-release": "6.2.2",
    "chai": "3.5.0",
    "mocha": "2.5.3",
    "jsdoc": "3.4.0",
    "istanbul": "0.4.4",
    "cz-conventional-changelog": "1.1.6"
  },
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.1"
  },
  "verb": {
    "ignore": [
      ".git"
    ],
    "deps": {
      "include": [
        "readme-includes",
        "readme-badges"
      ]
    },
    "reflinks": {
      "list": [
        "jsdiff",
        "option-cache",
        "template",
        "plasma",
        "config-cache"
      ]
    }
  },
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}