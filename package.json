{
  "name": "kronos-main",
  "version": "0.0.0-semantic-release",
  "main": "dist/index.js",
  "description": "Umbrella for all Kronos modules",
  "keywords": [],
  "contributors": [
    {
      "name": "Torsten Link",
      "email": "torstenlink@gmx.de"
    },
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "semantic-release": "semantic-release",
    "docs": "documentation readme {{module}} --section=API",
    "cover": "nyc --temp-directory build/nyc ava",
    "precover": "rollup -c tests/rollup.config.js",
    "test": "ava",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "prepare": "rollup -c",
    "lint": "documentation lint {{module}}"
  },
  "dependencies": {},
  "devDependencies": {
    "semantic-release": "^15.0.0",
    "markdown-doctest": "^0.9.1",
    "documentation": "^5.3.5",
    "nyc": "^11.4.1",
    "ava": "^0.25.0",
    "rollup": "^0.55.0",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-multi-entry": "^2.0.2"
  },
  "engines": {
    "node": ">=8.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kronos-Integration/kronos-main.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-main/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-main#readme",
  "ava": {
    "files": [
      "build/*-test.js"
    ],
    "presets": [
      "env"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/*-test.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "xo": {
    "space": true
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  }
}
