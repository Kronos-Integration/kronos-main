{
  "name": "kronos-main",
  "version": "0.0.0-semantic-release",
  "description": "Umbrella for all Kronos modules",
  "scripts": {
    "cover": "./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "test": "./node_modules/.bin/mocha tests/*_test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Kronos-Integration/kronos-main.git"
  },
  "bugs": {
    "url": "https://github.com/Kronos-Integration/kronos-main/issues"
  },
  "homepage": "https://github.com/Kronos-Integration/kronos-main#readme",
  "devDependencies": {
    "asciidoctor.js": "^1.5.5-4",
    "broccoli": "^1.1.1",
    "broccoli-funnel": "^1.1.0",
    "broccoli-merge-trees": "^1.2.1",
    "chai": "3.5.0",
    "cz-conventional-changelog": "2.0.0",
    "github-change-remote-file": "^4.2.0",
    "istanbul": "0.4.5",
    "js-yaml": "^3.8.1",
    "mocha": "^3.2.0",
    "node-github": "0.0.3",
    "octokat": "^0.6.2",
    "semantic-release": "6.3.2",
    "verb": "^0.8.10"
  },
  "contributors": [{
    "name": "Torsten Link",
    "email": "torstenlink@gmx.de"
  }, {
    "name": "Markus Felten",
    "email": "markus.felten@gmx.de"
  }],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.6"
  },
  "verb": {
    "ignore": [
      ".git"
    ],
    "deps": {
      "include": [
        "readme-includes",
        "readme-badges"
      ]
    },
    "reflinks": {
      "list": [
        "jsdiff",
        "option-cache",
        "template",
        "plasma",
        "config-cache"
      ]
    }
  },
  "keywords": [],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
